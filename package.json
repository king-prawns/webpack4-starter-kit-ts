{
  "name": "webpack4-starter-kit-ts",
  "version": "1.0.5",
  "description": "Webpack 4 starter kit TS",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "node ./config/dev",
    "build": "node ./config/prod",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "tc": "tsc --noEmit",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ava-ts",
    "prehttpserver": "yarn build",
    "httpserver": "http-server ./dist -p 8082 --silent"
  },
  "author": "Giorgio Gamberoni <giorgio.gamberoni@gmail.com> (https://github.com/king-prawns)",
  "license": "MIT",
  "homepage": "https://github.com/king-prawns/webpack4-starter-kit-ts#readme",
  "ava": {
    "files": [
      "./src/**/*test.ts"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/node": "10.12.9",
    "autoprefixer": "9.3.1",
    "ava": "0.25.0",
    "ava-ts": "0.25.2",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "1.0.1",
    "cssnano": "4.1.7",
    "cssnano-preset-advanced": "4.0.5",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "mini-css-extract-plugin": "0.4.4",
    "nyc": "13.1.0",
    "offline-plugin": "5.0.6",
    "opn": "5.4.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "ora": "3.0.0",
    "postcss-easy-import": "3.0.0",
    "postcss-load-config": "2.0.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.4.0",
    "postcss-reporter": "6.0.0",
    "postcss-responsive-type": "1.0.0",
    "resolve-url-loader": "3.0.0",
    "style-loader": "0.23.1",
    "stylelint": "9.8.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "terser-webpack-plugin": "1.1.0",
    "ts-loader": "5.3.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-loader": "3.6.0",
    "typescript": "3.1.6",
    "url-loader": "1.1.2",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {}
}
